{
  "name": "inventory-system-backend",
  "version": "1.0.0",
  "description": "Backend API for Inventory Management System",
  "main": "dist/server.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "postbuild": "node -e \"console.log('âœ… Build completed. Files in dist:'); require('fs').readdirSync('./dist').forEach(f => console.log('  -', f));\"",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:src": "prettier --write src/**/*.ts",
    "debug": "node -e \"console.log('Working directory:', process.cwd()); console.log('Files in current dir:', require('fs').readdirSync('.')); console.log('Dist exists:', require('fs').existsSync('./dist')); console.log('Server.js exists:', require('fs').existsSync('./dist/server.js'));\""
  },
  "keywords": [
    "inventory",
    "nodejs",
    "express",
    "typescript",
    "mongodb"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "multer": "^2.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.13",
    "@types/node": "^24.0.4",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.6.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
